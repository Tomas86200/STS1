import java.util.Date;


public class Exploitation {
	private int noExploitation;
	private Date dateDebut;
	
	public Exploitation(int noExploitation, Date dateDebut) {
		this.noExploitation = noExploitation;
		this.dateDebut = dateDebut;
	}

	public int getNoExploitation() {
		return noExploitation;
	}

	public void setNoExploitation(int noExploitation) {
		this.noExploitation = noExploitation;
	}

	public Date getDateDebut() {
		return dateDebut;
	}

	public void setDateDebut(Date dateDebut) {
		this.dateDebut = dateDebut;
	}
	
	
	
}
