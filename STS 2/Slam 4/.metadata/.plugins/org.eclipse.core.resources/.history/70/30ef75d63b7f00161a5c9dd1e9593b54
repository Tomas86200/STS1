
public abstract class TypeParcelle {

	private String classification;
	private IImpot unIImpot;

	public abstract float motantImpot (float surfaceClasseHa, float syrfaceTotHa, String dept);
	
	public TypeParcelle(String classification) {
		this.classification = classification;
	}

	public String getClassification() {
		return classification;
	}

	public void setClassification(String classification) {
		this.classification = classification;
	}

	@Override
	public String toString() {
		return "TypeParcel	le [classification=" + classification + "]";
	}

	public IImpot getUnIImpot() {
		return unIImpot;
	}

	public void setUnIImpot(IImpot unIImpot) {
		this.unIImpot = unIImpot;
	}
}
