import java.util.Date;


public class PersonnePhysique {
	private String prenom;
	private Date dateNaissance;
	private Exploitation uneExploitation;
	
	public PersonnePhysique(String prenom, Date dateNaissance,Exploitation uneExploitation) {
		this.prenom = prenom;
		this.dateNaissance = dateNaissance;
		this.uneExploitation = uneExploitation;
	}

	public String getPrenom() {
		return prenom;
	}

	public void setPrenom(String prenom) {
		this.prenom = prenom;
	}

	public Date getDateNaissance() {
		return dateNaissance;
	}

	public void setDateNaissance(Date dateNaissance) {
		this.dateNaissance = dateNaissance;
	}

	@Override
	public String toString() {
		return "PersonnePhysique [prenom=" + prenom + ", dateNaissance="
				+ dateNaissance + ", toString()=" + super.toString() + "]";
	}

	public Exploitation getUneExploitation() {
		return uneExploitation;
	}

	public void setUneExploitation(Exploitation uneExploitation) {
		this.uneExploitation = uneExploitation;
	}
	
	
}
