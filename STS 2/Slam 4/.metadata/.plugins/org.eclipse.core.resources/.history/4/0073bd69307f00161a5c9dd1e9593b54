
public class Impot implements IImpot {
	public Impot(){
		
	}

	public float getCoefB(String dept) {
		// TODO Auto-generated method stub
		return 0;
	}

	public float getCoefA(String classification) {
		// TODO Auto-generated method stub
		float coef;
		
		switch (classification){
			case "T01":
				coef = 190f;
				break;
			
			case "T02":
				coef = 160f;
				break;
				
			case "T03":
				coef = 130f;
				break;
				
			case "B01":
				coef = 110f;
				break;
				
			case "B02":
				coef = 100f;
				break;
				
			case "B03":
				coef = 95f;
				break;
				
			case "E01":
				coef = 220f;
				break;
				
			case "E02":
				coef = 210f;
				break;
				
			case "E03":
				coef = 200f;
				break;
				
			case "P01":
				coef = 150f;
				break;
				
			case "P02":
				coef = 140f;
				break;
				
			case "P03":
				coef = 120f;
				break;
				
			default:
				coef = 9999999999999f;
				break;
		}
		return coef;
	}

	public float getCoefC() {
		// TODO Auto-generated method stub
		return 0;
	}
	
}